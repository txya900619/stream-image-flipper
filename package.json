{
  "name": "@txya900619/stream-image-flipper",
  "version": "0.0.0",
  "description": "flip stream image",
  "author": "txya900619 @txya900619",
  "bin": {
    "stream-image-flipper": "./bin/run"
  },
  "homepage": "https://github.com/txya900619/stream-image-flipper",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "txya900619/stream-image-flipper",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "axios": "^0.27.2",
    "sharp": "^0.30.6"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/sharp": "^0.30.2",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "prettier-eslint": "^10.1.0",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4"
  },
  "oclif": {
    "bin": "stream-image-flipper",
    "dirname": "stream-image-flipper",
    "commands": "./dist/commands",
    "default": "download",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {},
    "helpClass": "./dist/help"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/txya900619/stream-image-flipper/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}